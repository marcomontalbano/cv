{
  "private": true,
  "name": "marcomontalbano-cv",
  "version": "0.2.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest --coverage",
    "build": "preact build --prerenderUrls ./prerender-urls.js",
    "dev:front": "preact watch --prerender --clear false --prerenderUrls ./prerender-urls.js",
    "dev:admin": "concurrently -n netlify-cms-proxy-server,admin \"npx netlify-cms-proxy-server\" \"npx serve src/static\"",
    "dev": "concurrently npm:dev:*",
    "prebuild:cv": "npm run build",
    "build:cv": "node -r tsm ./scripts/create-pdf.ts",
    "dep:upgrade:major": "npx npm-check-updates -u",
    "dep:upgrade:minor": "npm run dep:upgrade:major -- --target minor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcomontalbano/cv.git"
  },
  "keywords": [],
  "author": "Marco Montalbano <marcomontalbano.work@gmail.com> (https://marcomontalbano.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marcomontalbano/cv/issues"
  },
  "homepage": "https://github.com/marcomontalbano/cv#readme",
  "eslintIgnore": [
    "build/*",
    "dist/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@testing-library/preact": "~2.0.1",
    "@types/html-pdf": "~3.0.0",
    "@types/jest": "~27.4.1",
    "@types/node": "~17.0.23",
    "@types/showdown": "~1.9.4",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "concurrently": "^7.1.0",
    "date-fns": "~2.28.0",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "~7.29.4",
    "html-pdf": "~3.0.1",
    "husky": "~7.0.4",
    "jest": "~27.5.1",
    "lint-staged": "~12.3.7",
    "netlify-cms-json-exporter": "0.1.2",
    "sass": "^1.50.0",
    "preact": "^10.7.1",
    "preact-cli": "^3.3.5",
    "preact-render-to-string": "^5.1.21",
    "prettier": "~2.6.2",
    "sass-loader": "~10.2.1",
    "showdown": "~2.0.3",
    "ts-jest": "~27.1.4",
    "tsm": "^2.2.1",
    "typescript": "~4.6.3"
  }
