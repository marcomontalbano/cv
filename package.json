    "coverage/*",
    "dist/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "@testing-library/preact": "~3.2.3",
    "@types/node": "~20.12.7",
    "@types/showdown": "~2.0.6",
    "@typescript-eslint/eslint-plugin": "~7.6.0",
    "@typescript-eslint/parser": "~7.6.0",
    "concurrently": "^8.2.2",
    "date-fns": "~3.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "~7.34.1",
    "happy-dom": "^14.7.1",
    "netlify-cms-json-exporter": "0.1.2",
    "nodemon": "^3.1.0",
    "preact": "^10.20.2",
    "puppeteer": "^22.6.4",
    "sass": "^1.75.0",
    "showdown": "~2.1.0",
    "typescript": "~5.4.5",
    "vite": "^5.2.8",
    "vitest": "^1.5.0"
  }
